{
    "name": "cdn77/rabbitmq-bundle",
    "description": "RabbitMQ Symfony bundle for BunnyPHP.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "bunny/bunny": "^0.5.0",
        "myclabs/php-enum": "^1.8.4",
        "react/promise": "^2.0",
        "symfony/config": "^5.0 || ^6.0 || ^7.0",
        "symfony/console": "^5.0 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.0 || ^6.0 || ^7.0",
        "symfony/framework-bundle": "^5.0 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.1.5 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "cdn77/coding-standard": "^6.0",
        "ergebnis/composer-normalize": "^2.42",
        "infection/infection": "^0.29.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.10.66",
        "phpstan/phpstan-phpunit": "^1.3.16",
        "phpstan/phpstan-strict-rules": "^1.5.2",
        "phpunit/phpunit": "^9.5",
        "shipmonk/composer-dependency-analyser": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Cdn77\\RabbitMQBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77\\RabbitMQBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
